gp:AppliancesLighting gp:hasMeasure gp:Measure .
gp:Other gp:hasMeasure gp:Measure .
gp:Innovations gp:hasMeasure gp:Measure .

gp:Measure rdfs:label xsd:string ;
    gp:hasPossiblePoints xsd:integer ;
    gp:hasPointsAchieved xsd:integer ;
    gp:isRequired xsd:boolean .
           
gp:GreenPointRatedChecklist gp:hasLabel xsd:string ;
    gp:hasTargetedPoints xsd:integer ;
    gp:hasTotalAvailablePoints xsd:integer ;
    gp:hasMinimumPointsRequired xsd:integer ;
    gp:hasTotalPointsAchieved xsd:integer .
